@import('yeticss'); /* will still produce a single minified css file on build */
@import url('https://fonts.googleapis.com/css?family=Francois+One|Fjalla+One');
body
  font: 18px/1.6 'Fjalla One'

header
  padding-top: 50px
  font: 28px/1.4 'Francois One'

.label
  height: 40px

  .label-color
    width: 24px
    height: 24px
    border: 1px solid grey
    border-radius: 20px
    display: inline-block

  > span
    margin-left: 10px
    margin-right: 10px

a
  color: rebeccapurple
  text-decoration: none
  border-bottom: 1px solid gold

ul
  margin-left: -25px
  li
    list-style: none
    line-height:1.8
    span
      margin-right:5px
      vertical-align:1px